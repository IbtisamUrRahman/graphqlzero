{
  "name": "subocean-tiger",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "postbuild": "yarn netlify",
    "dev": "concurrently 'lerna run dev --stream --parallel' 'yarn netlify:dev'",
    "start": "lerna run start --stream --parallel",
    "netlify": "netlify-lambda build --config webpack.functions.js packages/subocean-tiger-graphql/dist/lambda",
    "netlify:dev": "netlify-lambda serve --config webpack.functions.js packages/subocean-tiger-graphql/dist/lambda"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "lerna": "^3.16.4",
    "netlify-lambda": "^1.6.3",
    "node-noop": "^1.0.0",
    "webpack": "4.39.2"
  }
}
